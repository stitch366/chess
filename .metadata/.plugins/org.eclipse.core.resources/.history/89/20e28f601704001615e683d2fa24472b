package application;

import java.io.IOException;

import javafx.fxml.FXML;
import javafx.fxml.FXMLLoader;
import javafx.scene.control.Label;
import javafx.scene.layout.AnchorPane;
import javafx.scene.layout.Region;

public class TurnIndoController {


	public TurnIndoController()
	{
		 FXMLLoader fxmlLoader = new FXMLLoader(getClass().getResource("/TurnIndicator.fxml"));
		 fxmlLoader.setRoot(this);
        fxmlLoader.setController(this);

        try {
            fxmlLoader.load();
        } catch (IOException exception) {
            throw new RuntimeException(exception);
        }
	}
	
    @FXML
    private Region BishopIcon;

    @FXML
    private Region KnightIcon;

    @FXML
    private Region QueenIcon;

    @FXML
    private AnchorPane turnLabel;

    @FXML
    private Region RookIcon;

    @FXML
    private Label MessageBox2;

    @FXML
    private Region KingIcon;

    @FXML
    private Region PawnIcon;

    @FXML
    private AnchorPane Image;

    @FXML
    private Label MessageBox;
	
}
