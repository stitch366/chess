import java.util.ArrayList;
import java.util.HashMap;

public class AttackBoard {
	boolean board[][] = new boolean[8][8];
	
	AttackBoard(ArrayList<String> enemyId, HashMap<String, Peice> enemy)
	{
		for(int x = 0; x < 8; x++)
		{
			for (int y = 0; y < 8; x++)
			{
				this.board[x][y]= false;
			}
		}
		int row;
		int col;
		ArrayList<String> spaces;
		for(int x = 0; x < enemyId.size(); x++)
		{
			spaces = enemy.get(enemyId.get(x)).getRange();
			for (int y = 0; y < spaces.size(); x++)
			{
				row = Integer.parseInt(spaces.get(y).split("-")[0]);
				col = Integer.parseInt(spaces.get(y).split("-")[1]);
				this.board[row][col]= false;
			}
		}
	}
}
